<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
      <appSettings>
        <!--Database connection string-->
        <add key="DBConStr" value="Data Source=.\SQLEXPRESS;database=AOSmith_WT;User id=sa; Password=ide123$%^;"/>




        <!--response timeout in milliseconds-->
        <add key="ResponseTimeout" value="500" />

        <add key="MODE" value="MASTER" />

        <!--Communication Layers-->
        <add key="COM_LAYERS" value="RS485"/>

        <!-- serial port settings-->
       <add key="PORT" value="COM13"/>

        <add key="SleepOnWrite" value="1"/>

        <!--Response Packet Fields-->
        <add key="RESPONSE_PACKET_FIELDS" value="DEPARTMENT"/>
        
        <!--Shift A Timings-->
        <add key="Shift_A_StartTime" value ="06:00:00"/>
        <add key="Shift_A_EndTime" value ="14:00:00"/>


        <!--Shift B Timings-->
        <add key="Shift_B_StartTime" value ="14:00:00"/>
        <add key="Shift_B_EndTime" value ="22:00:00"/>


        <!--Shift C Timings-->
        <add key="Shift_C_StartTime" value ="22:00:00"/>
        <add key="Shift_C_EndTime" value ="06:00:00"/>
      </appSettings>

</configuration>