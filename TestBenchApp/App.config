        <!--Printer Port-->
        <add key ="PRINTER_PORT" value="9100" />


        <!--SIMULATION-->
        <add key ="CONTROLLER_SIMULATION" value="No" />
        
     

        <!--SCANNER SIMULATION-->
        <add key ="SCANNER_SIMULATION" value="Yes" />
        
        
        <!--PRINTER SIMULATION-->
        <add key ="PRINTER_SIMULATION" value="No" />

        <!--ASSOCIATION TIMEOUT-->
        <add key ="ASSOCIATION_TIMEOUT" value="15" />


      </appSettings>

